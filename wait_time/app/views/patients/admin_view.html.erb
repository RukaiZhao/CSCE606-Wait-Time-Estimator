<h1 id="notice"><%= notice %></h1>
<h1 id="numOfPatients"><%= flash[:numOfPatients] %></h1>
<h1><%= @time1 %></h1>

<h1>Patients</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.name %></td>
        <td><%= patient.number %></td>
        <td><%= patient.time %></td>
        <!--<td><%= link_to 'Show', patient %></td>-->
        <td><%= link_to 'Edit', edit_patient_path(patient) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= button_to 'reset', button_path, method: :get, confirm: "Are you sure?" %>
<%= link_to 'back', new_patient_path %>

<br>

